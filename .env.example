# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/dm_sherpa?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Application
NODE_ENV=development
PORT=3000
API_PREFIX=api/v1

# JWT (for future auth)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRATION=7d

# Telephony Provider (Twilio example)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_DEFAULT_FROM=

# Print Vendor API
PRINT_VENDOR_API_URL=
PRINT_VENDOR_API_KEY=

# Feature Flags
ENABLE_SYNTHETIC_DATA=true
ENABLE_DIAGNOSTICS=true
ENABLE_EXPERIMENTS=true
